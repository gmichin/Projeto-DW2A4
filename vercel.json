{
  "version": 2,
  "rewrites": [
    { "source": "/login", "destination": "/login.html" },
    { "source": "/signup", "destination": "/signup.html" },
    { "source": "/profile", "destination": "/profile.html" },
    { "source": "/recommendedTracks", "destination": "/recommendedTracks.html" },
    { "source": "/topTracks", "destination": "/topTracks.html" },
    { "source": "/", "destination": "/index.html" },
    { "source": "/css/style.css", "destination": "/style.css" },
    { "source": "/users/:path*", "destination": "/routes/users.js" }
  ],
  "builds": [
    {
      "src": "/routes/**",
      "use": "@now/node-server"
    }
  ]
}
