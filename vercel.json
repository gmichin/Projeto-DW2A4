{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node-server"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/.*", "dest": "/users.js" },
      { "src": "/login", "dest": "/users.js" },
      { "src": "/profile", "dest": "/users.js" },
      { "src": "/signup", "dest": "/users.js" },
      { "src": "/topTracks", "dest": "/users.js" },
      { "src": "/recommendedTracks", "dest": "/users.js" }
    ]
  }
  